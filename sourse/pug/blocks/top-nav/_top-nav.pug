mixin top-nav() 
	// start top-nav
	+b.top-nav.block-with-lazy
		.container
			+e.row.row(data-aos="fade-up" data-aos-duration="700")
				+e.col-logo.col-auto
					a(href="https://hacks-ai.ru/ " target="_blank") 
						+img('img/svg/logo.svg').main-logo
				+e.col-logo.col-auto
					a(href="https://www.economy.gov.ru/ " target="_blank") 
						+img('img/svg/logo4.svg').logo-3
				+e.col-logo.col-auto
					a(href="https://rsv.ru/ " target="_blank") 
						+img('img/svg/logo2.svg').logo-2
				//- .col-auto.p-0.order-lg-0.order-last
				//- 	.menu-mobile.menu-mobile--js
				//- 		.menu-mobile__inner
				//- 			//- +menu-toggle()
				//- 			+mnu()
				.col-12.p-0.m-0.d-sm-none
				.col.col-md-auto.ms-md-auto
					+e.wrap
						+menu-toggle()
						+e.link.ms-auto.ms-md-0(href='https://lk.hacks-ai.ru/'): +icon('access')
						+e.link(href='https://lk.hacks-ai.ru/'): +icon('user')
	// end top-nav