mixin sAbout(data)
	// start sAbout
	+b.SECTION.sAbout.section#sAbout&attributes(attributes)
		.container
			+e.wrap
				//- .tech.tech-1 AI
				//- .tech.tech-2 Big Data
				//- .tech.tech-3 ML
				//- .tech.tech-4 Computer Vision
				picture
					source(media="(min-width: 992px)")
					img(src="", alt="").bg
				+e.date(data-aos="fade-up" data-aos-duration="700")
				.row
					.col-lg
						+e.content(data-aos="fade-up" data-aos-duration="700")
							.row
								.col-auto.order-sm-last
								//- .col
								//- 	+e.types-inner
							h1
							h2
							+e.text
							+e.row.row
								.col-auto
									+e.item
										+icon('user-solid')
										div
											p Участников
											.h2.count-attendee
								.col-auto
									+e.item
										+icon('users')
										div
											p Команд
											.h2.count-team
							//- +e.btn.btn.btn-dark(href='https://lk.hacks-ai.ru') Принять участие
					.col-lg-auto
						+e.info-wrap
	// end sAbout
